{
  "expo": {
    "name": "condy",
    "slug": "condy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "com.condy",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.condy",
      "supportsTablet": true
    },
    "android": {
      "package": "com.condy",
      "intentFilters": [
        {
          "action": "VIEW",
          "category": [
            "DEFAULT",
            "BROWSABLE"
          ],
          "data": {
            "scheme": "com.condy"
          }
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "googlePlacesApiKey": "AIzaSyDLCX6mp0wrb48jXfWk2goAvpEy-YUNwrw",
      "eas": {
        "projectId": "48a62ec1-11f9-45ba-a19f-7181e2c2edb6"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo precisa acessar suas fotos para upload de documentos.",
          "cameraPermission": "O aplicativo precisa acessar sua c√¢mera para tirar fotos de documentos."
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "string | string []",
          "enableGooglePay": "boolean"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./local/assets/notification_icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": [
            "./local/assets/notification_sound.wav",
            "./local/assets/notification_sound_other.wav"
          ],
          "enableBackgroundRemoteNotifications": false
        }
      ],
      "expo-document-picker",
      "expo-file-system",
      "expo-location"
    ],
    "owner": "diegopalazon"
  }
}
